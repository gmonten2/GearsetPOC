<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enrolled_Stage__c</fullName>
    <description>Used by Pardot for Onboarding email templates</description>
    <formula>IF(OR (ISPICKVAL(Current_Opportunity__r.StageName, &quot;Enrolled-Not Paid&quot;),(ISPICKVAL(Current_Opportunity__r.StageName, &quot;Enrolled-Paid&quot;))),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Enrolled Stage</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>

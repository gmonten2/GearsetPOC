<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>bt_stripe__Quantity_Remaining__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(ISNULL(bt_stripe__Quantity_Available__c), null, bt_stripe__Quantity_Available__c - NULLVALUE(bt_stripe__Quantity_Claimed__c, 0))</formula>
    <inlineHelpText>The number of remaining times this Code can be used. Calculated by subtracting Quantity Claimed from the Quantity Made Available. This field will be blank if the Quantity Made Available is blank.</inlineHelpText>
    <label>Quantity Remaining</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
